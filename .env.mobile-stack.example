NEXT_PUBLIC_SUPABASE_URL=https://<your-project-ref>.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=<your-anon-key>

# Supabase MCP + CLI
SUPABASE_PROJECT_REF=<your-project-ref>
SUPABASE_ACCESS_TOKEN=<your-personal-access-token>

# Mobile runtime keys
ONESIGNAL_APP_ID=<your-onesignal-app-id>
REVENUECAT_APPLE_API_KEY=<your-revenuecat-apple-key>
REVENUECAT_GOOGLE_API_KEY=<your-revenuecat-google-key>
REVENUECAT_WEBHOOK_SECRET=<your-revenuecat-webhook-secret>

# Optional feature toggles (string: true|false)
ENABLE_FIREBASE=true
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true

# Optional: GH Actions / Maestro
MAESTRO_API_KEY=
MAESTRO_PROJECT_ID=
